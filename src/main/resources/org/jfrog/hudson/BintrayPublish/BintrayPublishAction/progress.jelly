<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:l="/lib/layout"
        >
    <l:layout norefresh="true" title="Push to Bintray">
        <st:include it="${it.build}" page="sidepanel.jelly"/>
        <l:main-panel>
            <h1>Publishing ${it.build.displayName} to Bintray</h1>
            <p>${%Publish to Bintray is in progress:}</p>
            <st:include page="log.jelly"/>
        </l:main-panel>
    </l:layout>
</j:jelly>
